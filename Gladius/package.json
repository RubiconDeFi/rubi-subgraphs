{
    "name": "rubicon-v2",
    "version": "0.1.0",
    "author": "denver",
    "scripts": {
        "test": "graph test",
        "coverage": "graph test -- -c",
        "prepare:arbitrum": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
        "prepare:arbitrum:goerli": "mustache config/arbitrum_goerli.json subgraph.template.yaml > subgraph.yaml",
        "prepare:optimism": "mustache config/optimism.json subgraph.template.yaml > subgraph.yaml",
        "codegen": "graph codegen",
        "build": "graph build",
        "deploy:arbitrum": "graph deploy --product hosted-service etherealiska/gladius-arbitrum",
        "deploy:arbitrum:goerli": "graph deploy --product hosted-service etherealiska/gladius-arbitrum-goerli",
        "deploy:optimism": "graph deploy --product hosted-service etherealiska/gladius-optimism-v3",
        "create-local": "graph create --node http://localhost:8020/ Gladius_V2",
        "remove-local": "graph remove --node http://localhost:8020/ Gladius_V2",
        "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 Gladius_V2",
        "deploy-forked": "graph deploy Gladius --node http://localhost:8020/ --ipfs http://localhost:5001",
        "create:cloud:arbitrum": "source ../.env && graph create --node $GRAPH_NODE_URL Gladius_Arbitrum",
        "remove:cloud:arbitrum": "source ../.env && graph remove --node $GRAPH_NODE_URL Gladius_Arbitrum",
        "deploy:cloud:arbitrum": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Gladius_Arbitrum",
        "create:cloud:arbitrum:goerli": "source ../.env && graph create --node $GRAPH_NODE_URL Gladius_Arbitrum_Goerli",
        "remove:cloud:arbitrum:goerli": "source ../.env && graph remove --node $GRAPH_NODE_URL Gladius_Arbitrum_Goerli",
        "deploy:cloud:arbitrum:goerli": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Gladius_Arbitrum_Goerli",
        "create:cloud:optimism": "source ../.env && graph create --node $GRAPH_NODE_URL Gladius_Optimism_V2",
        "remove:cloud:optimism": "source ../.env && graph remove --node $GRAPH_NODE_URL Gladius_Optimism_V2",
        "deploy:cloud:optimism": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Gladius_Optimism_V2"
    },
    "devDependencies": {
        "@graphprotocol/graph-cli": "^0.48.0",
        "@graphprotocol/graph-ts": "^0.29.3",
        "as-bigint": "^0.5.3",
        "dotenv": "^16.0.3",
        "matchstick-as": "^0.5.2",
        "mustache": "^3.1.0"
    }
}
