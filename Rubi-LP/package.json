{
    "name": "rubicon-v2",
    "version": "0.1.0",
    "author": "denver",
    "scripts": {
        "test": "graph test",
        "coverage": "graph test -- -c",
        "prepare:arbitrum": "mustache config/arbitrum.json subgraph.template.yaml > subgraph.yaml",
        "prepare:arbitrum:sepolia": "mustache config/arbitrum_sepolia.json subgraph.template.yaml > subgraph.yaml",
        "prepare:optimism": "mustache config/optimism.json subgraph.template.yaml > subgraph.yaml",
        "prepare:ethereum": "mustache config/ethereum.json subgraph.template.yaml > subgraph.yaml",
        "prepare:base": "mustache config/base.json subgraph.template.yaml > subgraph.yaml",
        "codegen": "graph codegen",
        "build": "graph build",
        "create-local": "graph create --node http://localhost:8020/ Rubi_LP",
        "remove-local": "graph remove --node http://localhost:8020/ Rubi_LP",
        "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 Rubi_LP",
        "deploy-forked": "graph deploy Gladius --node http://localhost:8020/ --ipfs http://localhost:5001",
        "create:cloud:arbitrum": "source ../.env && graph create --node $GRAPH_NODE_URL Rubi_LP_Arbitrum",
        "remove:cloud:arbitrum": "source ../.env && graph remove --node $GRAPH_NODE_URL Rubi_LP_Arbitrum",
        "deploy:cloud:arbitrum": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Rubi_LP_Arbitrum",
        "create:cloud:arbitrum:sepolia": "source ../.env && graph create --node $GRAPH_NODE_URL Rubi_LP_Arbitrum_Sepolia",
        "remove:cloud:arbitrum:sepolia": "source ../.env && graph remove --node $GRAPH_NODE_URL Rubi_LP_Arbitrum_Sepolia",
        "deploy:cloud:arbitrum:sepolia": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Rubi_LP_Arbitrum_Sepolia",
        "create:cloud:optimism": "source ../.env && graph create --node $GRAPH_NODE_URL Rubi_LP_Optimism",
        "remove:cloud:optimism": "source ../.env && graph remove --node $GRAPH_NODE_URL Rubi_LP_Optimism",
        "deploy:cloud:optimism": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Rubi_LP_Optimism",
        "create:cloud:ethereum": "source ../.env && graph create --node $GRAPH_NODE_URL Rubi_LP_Ethereum",
        "remove:cloud:ethereum": "source ../.env && graph remove --node $GRAPH_NODE_URL Rubi_LP_Ethereum",
        "deploy:cloud:ethereum": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Rubi_LP_Ethereum",
        "create:cloud:base": "source ../.env && graph create --node $GRAPH_NODE_URL Rubi_LP_Base",
        "remove:cloud:base": "source ../.env && graph remove --node $GRAPH_NODE_URL Rubi_LP_Base",
        "deploy:cloud:base": "source ../.env && graph deploy --node $GRAPH_NODE_URL --ipfs $IPFS_URL Rubi_LP_Base"
    },
    "devDependencies": {
        "@graphprotocol/graph-cli": "^0.48.0",
        "@graphprotocol/graph-ts": "^0.29.3",
        "mustache": "^3.1.0",
        "as-bigint": "^0.5.3"
    }
}
